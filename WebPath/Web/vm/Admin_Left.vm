	line-height:34px;
	display:block;
	color:#FFF;
	font-weight:bold;
	text-decoration: none;
	text-align:center;
}
.left_bg{
	width:150px;
}
.menu_list{
	margin:0px auto;
	padding:0px;
	padding-top:0px;
	padding-left:0px;
}
.menu_list ul{
	margin:0px;
	padding:0px;
	background-color:#DDEEFF;
}
.menu_list li{
	list-style-type:none;
	padding:2px;
	padding-top:1px;
	padding-bottom:1px;
	font-size:14px;
	color:#333333;
	font-weight:bold;
	border-bottom-width: 1px;
	border-bottom-style: dotted;
	border-bottom-color: #999999;
	background-color:#DDEEFF;
}
.menu_list li a{
	list-style-type:none;
	padding:3px;
	font-size:12px;
	color:#333333;
	width:140px;
	display:block;
    padding-top:6px;
	padding-left:44px;
	background-image:url(../cdn/images/page_00.gif);
	background-repeat:no-repeat;
	background-position:12px 4px;
}
.menu_list li a:hover{
	list-style-type:none;
	padding:3px;
	font-size:12px;
	color:#0033CC;
	width:140px;
	display:block;
    padding-top:6px;
	padding-left:44px;
	text-decoration: none;
	background-image:url(../cdn/images/page_01.gif);
	background-repeat:no-repeat;
	background-position:12px 4px;
	background-color:#FFF;
	

}
.menu_body {
	display:none;
}
.menu_head{
	padding: 5px 10px;
	cursor: pointer;
	position: relative;
	margin:1px;
    font-weight:bold;
	background-image:url(../cdn/images/menu_tag_bg1.gif);
	background-repeat:no-repeat;
	padding-left:32px;
}
.menu_head a{
	font-weight: bold;
	color: #333333;
	text-decoration: none;
}
.menu_tab ul{
	background-color:#FFFFFF;
}
</style>
<script type="text/javascript">
    $(document).ready(function () {

        $("#firstpane p.menu_head").click(function () {
            $(this).css({ backgroundImage: "url(../cdn/images/menu_tag_bg1.gif)" }).next("div.menu_body").slideToggle(300).siblings("div.menu_body").slideUp("slow");
            $(this).siblings().css({ backgroundImage: "url(../cdn/images/menu_tag_bg1.gif)" });
        });
    });
    function MenuOpen() {

        document.getElementById("menu_1").style.display = document.getElementById("menu_1").style.display == 'block' ? 'none' : 'block';

    }
</script>

</head>

<body style="overflow-x:hidden" onload="MenuOpen()">
